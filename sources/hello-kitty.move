module hello_kitty::hello_kitty {
    use sui::coin;
    use sui::url::new_unsafe_from_bytes;

    /// The OTW for the Coin
    public struct HELLO_KITTY has drop {}

    const DECIMALS: u8 = 9;
    const SYMBOL: vector<u8> = b"Hello Kitty";
    const DESCRIPTION: vector<u8> = b"HelloKitty";
    const URL: vector<u8> = b"data:image/jpeg;base64,";
    const MINT_AMOUNT: u64 = 19052024000000000;
    const TREASURY_CAP_RECIPIENT: address = @0x0;

    /// Init the Coin
    fun init(witness: HELLO_KITTY, ctx: &mut TxContext) {
        let (mut treasury, metadata) = coin::create_currency(
            witness, 
            DECIMALS, 
            SYMBOL, 
            SYMBOL, 
            DESCRIPTION, 
            option::some(new_unsafe_from_bytes(URL)), 
            ctx
        );

        coin::mint_and_transfer(&mut treasury, MINT_AMOUNT, tx_context::sender(ctx), ctx);

        transfer::public_transfer(treasury, TREASURY_CAP_RECIPIENT);
        transfer::public_share_object(metadata);
    }
}
